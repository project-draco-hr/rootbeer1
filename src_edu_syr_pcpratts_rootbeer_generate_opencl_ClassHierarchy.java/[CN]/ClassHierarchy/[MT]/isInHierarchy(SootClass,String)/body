{
  if (class_name.equals("java.lang.Object"))   return false;
  if (next.getName().equals(class_name))   return true;
  SootClass curr=next;
  while (true) {
    if (curr.isLibraryClass())     return false;
    if (hasSuperClass(curr) == false)     return false;
    SootClass superclass=curr.getSuperclass();
    if (superclass.getName().equals(class_name))     return true;
    curr=superclass;
  }
}
