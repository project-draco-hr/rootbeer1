{
  ArrayMultApp app=new ArrayMultApp();
  int[] array=new int[10];
  for (int i=0; i < array.length; ++i) {
    array[i]=i;
  }
  for (int i=0; i < array.length; ++i) {
    System.out.println("start array[" + i + "]: "+ array[i]);
  }
  app.multArray(array);
  for (int i=0; i < array.length; ++i) {
    System.out.println("final array[" + i + "]: "+ array[i]);
  }
}
