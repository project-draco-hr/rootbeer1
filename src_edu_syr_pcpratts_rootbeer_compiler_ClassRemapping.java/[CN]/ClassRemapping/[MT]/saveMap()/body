{
  String filename="src" + mapFilename();
  try {
    PrintWriter writer=new PrintWriter(filename);
    Iterator<String> iter=m_clonedMap.keySet().iterator();
    while (iter.hasNext()) {
      String key=iter.next();
      String value=m_clonedMap.get(key);
      writer.println(key + " => " + value);
    }
    writer.flush();
    writer.close();
  }
 catch (  Exception ex) {
    ex.printStackTrace();
  }
}
