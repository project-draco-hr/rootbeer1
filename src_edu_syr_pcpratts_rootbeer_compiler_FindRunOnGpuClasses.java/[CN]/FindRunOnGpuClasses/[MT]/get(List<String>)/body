{
  List<String> ret=new ArrayList<String>();
  for (  String cls : app_classes) {
    try {
      SootClass soot_class=RootbeerScene.v().getClass(cls);
      if (matches(soot_class)) {
        ret.add(soot_class.getName());
      }
    }
 catch (    RuntimeException ex) {
      continue;
    }
  }
  return ret;
}
