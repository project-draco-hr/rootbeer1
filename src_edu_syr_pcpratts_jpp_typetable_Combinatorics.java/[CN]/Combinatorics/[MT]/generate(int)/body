{
  List<List<Integer>> ret=new ArrayList<List<Integer>>();
  if (size == 1) {
    List<Integer> zero=new ArrayList<Integer>();
    zero.add(0);
    ret.add(zero);
    List<Integer> one=new ArrayList<Integer>();
    one.add(1);
    ret.add(one);
    return ret;
  }
  for (int i=0; i < Math.pow(size,size); ++i) {
    List<Integer> curr=baseN(i,size);
    addZeros(curr,size);
    ret.add(curr);
  }
  return ret;
}
