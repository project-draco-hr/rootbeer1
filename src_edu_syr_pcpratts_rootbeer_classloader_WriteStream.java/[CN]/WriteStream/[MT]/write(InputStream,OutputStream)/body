{
  while (true) {
    int read_len=4096;
    byte[] buffer=new byte[read_len];
    int curr_read=fin.read(buffer);
    if (curr_read == -1) {
      break;
    }
    byte[] to_write=new byte[curr_read];
    for (int i=0; i < curr_read; ++i) {
      to_write[i]=buffer[i];
    }
    fout.write(to_write);
  }
}
