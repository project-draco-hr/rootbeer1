{
  String two=m_CurrChar.getString() + m_NextChar.getString();
  if (two.equals("/*")) {
    m_InComment=true;
    m_SpecialEndToken="*/";
    return true;
  }
 else   if (m_CurrChar.getString().equals("\"") && m_InComment == false) {
    m_SpecialEndToken="\"";
    return true;
  }
 else   if (m_CurrChar.getString().equals("'") && m_InComment == false) {
    m_SpecialEndToken="\'";
    return true;
  }
  return false;
}
