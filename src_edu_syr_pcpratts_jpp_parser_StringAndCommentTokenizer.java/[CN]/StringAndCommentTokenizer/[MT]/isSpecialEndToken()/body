{
  String curr;
  if (m_SpecialEndToken.length() == 2) {
    curr=m_CurrChar.getString() + m_NextChar.getString();
  }
 else {
    curr=m_CurrChar.getString();
  }
  if (curr.equals(m_SpecialEndToken)) {
    if (m_SpecialEndToken.equals("*/")) {
      m_InComment=false;
    }
    return true;
  }
  return false;
}
