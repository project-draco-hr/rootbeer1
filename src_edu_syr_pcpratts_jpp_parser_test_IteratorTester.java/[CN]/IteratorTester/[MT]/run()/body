{
  try {
    TestFilesListFactory factory=new TestFilesListFactory();
    List<String> files=factory.create();
    for (    String file : files) {
      if (testFile(file)) {
        writeToFile(file);
        factory.setPassed(file);
      }
 else {
        return;
      }
    }
  }
 catch (  Exception ex) {
    ex.printStackTrace();
    return;
  }
}
