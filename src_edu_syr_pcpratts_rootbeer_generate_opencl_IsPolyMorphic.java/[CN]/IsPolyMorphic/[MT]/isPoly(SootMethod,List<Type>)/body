{
  if (hierarchy.size() > 2) {
    return true;
  }
  if (hierarchy.size() == 1) {
    return false;
  }
  String sub_sig=soot_method.getSubSignature();
  SootClass obj_class=Scene.v().getSootClass("java.lang.Object");
  if (obj_class.declaresMethod(sub_sig)) {
    return true;
  }
  return false;
}
