{
  m_bcl.top().openClass(m_runtimeBasicBlock);
  m_bcl.top().startMethod("getNewUsed",BooleanType.v());
  boolean new_used=OpenCLScene.v().getNewUsed();
  int new_used_int;
  if (new_used) {
    new_used_int=1;
  }
 else {
    new_used_int=0;
  }
  m_bcl.top().returnValue(IntConstant.v(new_used_int));
  m_bcl.top().endMethod();
}
