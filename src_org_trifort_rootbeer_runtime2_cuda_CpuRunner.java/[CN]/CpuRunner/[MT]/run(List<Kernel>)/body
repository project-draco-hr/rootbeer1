{
  int items_per=cpu_jobs.size() / m_NumCores;
  for (int i=0; i < m_NumCores; ++i) {
    int end_index;
    if (i == m_NumCores - 1) {
      end_index=cpu_jobs.size();
    }
 else {
      end_index=(i + 1) * items_per;
    }
    List<Kernel> jobs=cpu_jobs.subList(i * items_per,end_index);
    m_Cores.get(i).put(jobs);
  }
}
