{
  element=10;
  element1=new float[10];
  element2=new float[10][10];
  element3=new float[10][10][10];
  for (int m=0; m < 10; m++) {
    for (int n=0; n < 10; n++) {
      for (int p=0; p < 10; ++p) {
        element3[m][n][p]=(float)p;
        element2[n][p]=(float)p;
        element1[p]=(float)p;
      }
    }
  }
}
