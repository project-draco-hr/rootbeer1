{
  element=10;
  element1=new short[10];
  element2=new short[10][10];
  element3=new short[10][10][10];
  for (int m=0; m < 10; m++) {
    for (int n=0; n < 10; n++) {
      for (int p=0; p < 10; ++p) {
        element3[m][n][p]=(short)p;
        element2[n][p]=(short)p;
        element1[p]=(short)p;
      }
    }
  }
}
