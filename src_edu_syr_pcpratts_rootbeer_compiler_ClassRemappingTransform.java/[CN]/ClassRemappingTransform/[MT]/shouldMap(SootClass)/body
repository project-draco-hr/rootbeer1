{
  if (m_AppClass) {
    if (m_ClassRemapping.containsKey(soot_class.getName())) {
      setModified();
      return true;
    }
 else {
      return false;
    }
  }
 else {
    if (m_ClassRemapping.containsKey(soot_class.getName()) && !m_ClassRemapping.cloned(m_CurrClass)) {
      m_HasNext=true;
      m_ClassRemapping.cloneClass(m_CurrClass);
    }
    return false;
  }
}
