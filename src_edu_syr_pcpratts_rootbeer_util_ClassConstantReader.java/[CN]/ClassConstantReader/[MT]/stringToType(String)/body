{
  int dims=0;
  while (value.charAt(0) == '[') {
    dims++;
    value=value.substring(1);
  }
  if (dims != 0 && value.charAt(0) == 'L') {
    value=value.substring(1,value.length() - 2);
  }
  value=value.replace("/",".");
  Type base_type=getType(value);
  if (dims != 0) {
    return ArrayType.v(base_type,dims);
  }
 else {
    return base_type;
  }
}
