{
  for (int i=0; i < m_ccpu.length; ++i) {
    int cpu_value=m_ccpu[i];
    int gpu_value=m_cgpu[i];
    if (cpu_value != gpu_value) {
      StringBuilder builder=new StringBuilder();
      builder.append("Verfiy Failed at: ");
      builder.append(i);
      builder.append("\n");
      builder.append("  cpu_value: ");
      builder.append(cpu_value);
      builder.append("\n");
      builder.append("  gpu_value: ");
      builder.append(gpu_value);
      builder.append("\n");
      m_errorMessage=builder.toString();
      return false;
    }
  }
  return true;
}
