{
  for (  SootMethod method : m_methods) {
    if (method.hasActiveBody()) {
      Body body=method.getActiveBody();
      List<UnitBox> boxes=body.getAllUnitBoxes();
      for (      UnitBox box : boxes) {
        Unit unit=box.getUnit();
        if (unit instanceof NewExpr) {
          return true;
        }
      }
    }
  }
  return false;
}
