{
  if (m_sootMethod.getName().equals("<init>"))   return "";
  if (m_sootMethod.isConcrete() == false) {
    return "";
  }
  List<String> decls=getMethodDecls();
  StringBuilder ret=new StringBuilder();
  for (  String decl : decls) {
    ret.append(getMethodBody(decl));
  }
  return ret.toString();
}
