{
  int i=start;
  int k=end;
  if (end - start >= 1) {
    int pivot=array[start];
    while (k > i) {
      while (array[i] <= pivot && i <= end && k > i) {
        i++;
      }
      while (array[k] > pivot && k >= start && k >= i) {
        k--;
      }
      if (k > i) {
        swap(array,i,k);
      }
    }
    swap(array,start,k);
    quicksort(array,start,k - 1);
    quicksort(array,k + 1,end);
  }
 else {
    return;
  }
}
