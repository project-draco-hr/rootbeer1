{
  Value condition;
  if (op.equals("==")) {
    condition=jimple.newEqExpr(lhs,rhs);
  }
 else   if (op.equals("!=")) {
    condition=jimple.newNeExpr(lhs,rhs);
  }
 else   if (op.equals(">=")) {
    condition=jimple.newGeExpr(lhs,rhs);
  }
 else {
    throw new UnsupportedOperationException();
  }
  mAssembler.addIf(condition,target_label);
}
