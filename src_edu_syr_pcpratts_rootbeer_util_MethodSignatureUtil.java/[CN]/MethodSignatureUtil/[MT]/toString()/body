{
  StringBuilder ret=new StringBuilder();
  ret.append("<");
  ret.append(m_className);
  ret.append(": ");
  ret.append(m_returnType);
  ret.append(" ");
  ret.append(m_methodName);
  ret.append("(");
  for (int i=0; i < m_params.size(); ++i) {
    ret.append(m_params.get(i));
    if (i < m_params.size() - 1) {
      ret.append(",");
    }
  }
  ret.append(")>");
  return ret.toString();
}
