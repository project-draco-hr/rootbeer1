{
  try {
    SootClass cls=new SootClass(name);
    m_Fixups.add(name);
    CoffiClassSource source=new CoffiClassSource(name,is);
    Dependencies deps=source.resolve(cls);
    cls.setApplicationClass();
    Set types=deps.typesToSignature;
    Iterator<String> iter=types.iterator();
    while (iter.hasNext()) {
      Object next=iter.next();
      String class_name;
      if (next instanceof String)       class_name=(String)next;
 else       if (next instanceof Type) {
        class_name=extractClassName((Type)next);
        if (class_name.equals(""))         continue;
      }
 else       continue;
      if (!m_Dependencies.contains(class_name) && !m_Classes.containsKey(class_name))       m_Dependencies.add(class_name);
    }
    return cls;
  }
 catch (  Exception ex) {
    throw new RuntimeException(ex);
  }
 finally {
    try {
      if (is != null)       is.close();
    }
 catch (    Exception ex) {
      ex.printStackTrace();
    }
  }
}
