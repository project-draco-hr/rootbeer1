{
  while (true) {
    KernelLaunch item=m_toThread.take();
    try {
      if (item.quit()) {
        m_fromThread.put(item);
        return;
      }
      cudaRun(item.getDeviceIndex(),item.getCubinFile(),item.getCubinLength(),item.getBlockShapeX(),item.getGridShapeX(),item.getNumThreads(),item.getObjectMem(),item.getHandlesMem(),item.getExceptionsMem(),item.getClassMem(),b2i(item.getUsingKernelTemplates()),b2i(Configuration.runtimeInstance().getExceptions()));
      m_fromThread.put(item);
    }
 catch (    Exception ex) {
      item.setException(ex);
      m_fromThread.put(item);
    }
  }
}
