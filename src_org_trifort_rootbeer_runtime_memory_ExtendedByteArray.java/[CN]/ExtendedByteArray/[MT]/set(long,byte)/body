{
  int array=(int)(index / m_MaxSize);
  int offset=(int)(index % m_MaxSize);
  byte[] data=m_Data.get(array);
  try {
    data[offset]=value;
  }
 catch (  ArrayIndexOutOfBoundsException ex) {
    System.out.println("invalid: " + index + " "+ array+ " "+ offset+ " "+ data.length+ " "+ m_Size+ " "+ m_MaxSize);
    throw ex;
  }
}
